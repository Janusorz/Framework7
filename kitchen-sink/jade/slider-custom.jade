.navbar
  .navbar-inner
    .left.sliding
      a(href="index.html").back.link
        i.icon.icon-back
        span Back
    .center.sliding Custom Controls
    .right
      a(href="#").open-panel.link.icon-only
        i.icon.icon-bars
.pages.navbar-through
  .page(data-page="slider-custom").no-toolbar
    .page-content
      .ks-slider-custom
        .swiper-container.swiper-init(data-pagination=".swiper-pagination", data-spaceBetween="0", data-nextButton=".swiper-next-button", data-prevButton=".swiper-prev-button", data-indexButton=".swiper-pagination-bullet", data-paginationHide="false")
          .swiper-pagination
          .swiper-wrapper
            - for (var i = 1; i < 8; i++)
              .swiper-slide(style="background-image:url(http://hhhhold.com/1024x1024/d/jpg?#{i})")
        a(href="#").swiper-prev-button
          i.icon.icon-prev
        a(href="#").swiper-next-button
          i.icon.icon-next
