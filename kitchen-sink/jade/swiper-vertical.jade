.navbar
  .navbar-inner
    .left.sliding
      a(href="index.html").back.link
        i.icon.icon-back
        span Back
    .center.sliding Slider Vertical
    .right
      a(href="#").open-panel.link.icon-only
        i.icon.icon-bars
.pages.navbar-through
  .page(data-page="slider-vertical")
    .page-content
      .swiper-container.swiper-init.ks-demo-slider(data-direction="vertical", data-pagination=".swiper-pagination", data-paginationHide="true")
        .swiper-pagination
        .swiper-wrapper
          - for (var i = 1; i < 11; i++)
            .swiper-slide Slide #{i}